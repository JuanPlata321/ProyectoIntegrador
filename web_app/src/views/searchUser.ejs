<%- include('partials/_header'); %>

<section class="header">
    <div id="searchitemPanel">
        <h1>Usuarios asociados al sistema de la concesionaria</h1>
        <table id="articleTable" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>CÉDULA</th>
                    <th>NOMBRES</th>                                
                    <th>APELLIDOS</th>
                    <th>CORREO ELECTRÓNICO</th>
                    <th>CONTACTO</th>                                
                    <th>DIRECCCIÓN</th>
                    <th>AREA ASOCIADA</th>
                    <th>ESTADO</th>                                             
                </tr>
            </thead>
            <tbody>
                <% if (data) { %>
                    <% for (var i = 0; i < data.length; i++) { %>
                    <tr>
                        <td><%= data[i].ID_EMPLEADO %></td>
                        <td><%= data[i].CEDULA %></td>
                        <td><%= data[i].NOMBRES %></td>
                        <td><%= data[i].APELLIDOS %></td>
                        <td><%= data[i].CORREO_ELECTRONICO %></td>
                        <td><%= data[i].CONTACTO %></td>
                        <td><%= data[i].DIRECCION %></td>
                        <td><%= data[i].DEPARTAMENTO_AREA_ID_DEPARTAMENTO_AREA %></td>
                        <td><%= data[i].ESTADO_EMPLEADO_ID_ESTADO_EMPLEADO %></td>
                    </tr>
                    <% } %>
                <% } %>
            </tbody>
        </table>
    </div>
</section>

<%- include('partials/_footer'); %>