<%- include('partials/_header'); %>

<section class="header">
    <div id="searchitemPanel">
        <h1>Registro del movimiento de los vehículos</h1>
        <table id="articleTable" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>FECHA DE REGISTRO</th>
                    <th>DESCRIPCIÓN</th>                                
                    <th>VEHÍCULO ASOCIADO</th>                                 
                </tr>
            </thead>
            <tbody>
                <% if (data) { %>
                    <% for (var i = 0; i < data.length; i++) { %>
                    <tr>
                        <td><%= data[i].ID_REGISTRO_MOVIMIENTO_VEHICULOS %></td>
                        <td><%= data[i].FECHA_REGISTRO %></td>
                        <td><%= data[i].DESCRIPCION %></td>
                        <td><%= data[i].VEHICULOS_ID_VEHICULOS %></td>
                    </tr>
                    <% } %>
                <% } %>
            </tbody>
        </table>
    </div>
</section>

<%- include('partials/_footer'); %>